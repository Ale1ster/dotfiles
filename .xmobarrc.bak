Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
	   , commands = [ Run Cpu ["--Low","6", "--High","50", "--normal","green", "--high","red", "--template","<fc=#1c8c1c>Cpu</fc>: <total>%"] 10
                    , Run Memory ["--template","<fc=#1c8c1c>Mem</fc>: <usedratio>%"] 10
                    , Run Swap ["--template","<fc=#1c8c1c>Swap</fc>: <usedratio>%"] 10
                    , Run Network "wlp3s0" ["--Low","80", "--High","200", "--normal","green", "--high","red", "--suffix","False", "--template","<fc=#1c8c1c><dev></fc>: <rx>Kb/s - <tx>Kb/s"] 20
                    , Run Date "%a %b %_d %l:%M" "date" 300
					, Run Battery ["-t","<fc=#1c8c1c>Bat</fc>: <left>% / <timeleft>","-L","50","-H","75","-h","green","-n","yellow","-l","red"] 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %battery% | %cpu% <fc=#0080ff>|</fc> %memory% * %swap% <fc=#0080ff>|</fc> %wlp3s0% <fc=#0080ff>|</fc> <fc=#ee9a00>%date%</fc>"
       }
